{
	// "terminal.integrated.inheritEnv": false,
	"editor.fontSize": 17,
	"editor.formatOnPaste": true,
	"editor.wordWrap": "on",
	"workbench.activityBar.visible": true,
	"prettier.useTabs": true,
	"editor.minimap.enabled": false,
	"workbench.startupEditor": "newUntitledFile",
	"prettier.htmlWhitespaceSensitivity": "ignore",
	"editor.formatOnSave": true,
	"terminal.integrated.defaultProfile.osx": "zsh",
	"liveServer.settings.donotShowInfoMsg": true,
	"vsicons.dontShowNewVersionMessage": true,
	"workbench.preferredDarkColorTheme": "Gruvbox Dark (Medium)",
	"workbench.preferredHighContrastColorTheme": "Gruvbox Dark (Medium)",
	"workbench.preferredLightColorTheme": "Gruvbox Light (Hard)",
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"python.formatting.provider": "black",
	"python.languageServer": "Jedi",
	"python.pythonPath": "/Library/Frameworks/Python.framework/Versions/3.10/bin/python3",
	"python.defaultInterpreterPath": "/Library/Frameworks/Python.framework/Versions/3.10/bin/python3",
	"php.validate.executablePath": "/opt/homebrew/opt/php@7.4/bin:$PATH",
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	 "prettier.printWidth": 300,
	"prettier.singleQuote": true,
	"explorer.confirmDelete": false,
	"editor.cursorStyle": "line",
	"editor.cursorWidth": 3,
	"editor.cursorBlinking": "expand",
	"editor.renderWhitespace": "none",
	"mjml.autoPreview": true,
	"mjml.beautifyHtmlOutput": true,
	"explorer.compactFolders": false,
	"editor.tokenColorCustomizations": {
		"comments": "#696B62",
		"functions": "#C1DB99",
		"types": "#BC8A82",
		"strings": "#08FFC8",
		"numbers": "#8ABC82",
		// "variables": "#FF0000"
		//  "keywords": "#9be466",
		"textMateRules": [
			//JavaScript
			{
				"scope": "keyword.operator",
				"settings": {
					"foreground": "#FDFBF9"
					// "#C1DB99" Light Pastel Green
				}
			},
			{
				"scope": "variable.other.object.js",
				"settings": {
					"foreground": "#9be466"
				}
			},
			{
				"scope": "string.quoted.double.json.comments",
				"settings": {
					"foreground": "#08FFC8"
				}
			},
			{
				"scope": "string.quoted.double.json",
				"settings": {
					"foreground": "#9CF196"
				}
			},
			{
				"scope": "variable.other.readwrite.js",
				"settings": {
					"foreground": "#08FFC8"
				}
			},
			//CSS
			{
				"scope": "entity.name.tag.css",
				"settings": {
					"foreground": "#9be466"
				}
			},
			{
				"scope": "keyword.other.unit.px.css",
				"settings": {
					"foreground": "#F8F2EB"
				}
			},
			{
				"scope": "keyword.other.unit.percentage.css",
				"settings": {
					"foreground": "#F8F2EB"
				}
			},
			{
				"scope": "entity.name.tag.wildcard.css",
				"settings": {
					"foreground": "#08FFC8"
				}
			},
			//SCSS
			{
				"scope": "entity.name.tag.reference.scss",
				"settings": {
					"foreground": "#08FFC8"
				}
			},
			//HTML
			{
				"scope": "entity.name.tag.inline.any.html",
				"settings": {
					"foreground": "#7579E7"
				}
			},
			{
				"scope": "entity.name.tag.block.any.html",
				"settings": {
					"foreground": "#D2E603"
				}
			},
			{
				"scope": "entity.name.tag.structure.any.html",
				"settings": {
					"foreground": "#28DF99"
				}
			},
			{
				"scope": "entity.name.tag.other.html",
				"settings": {
					"foreground": "#28DF99"
				}
			},
			{
				"scope": "entity.name.tag.script.html",
				"settings": {
					"foreground": "#28DF99"
				}
			},
			//PHP
			{
				"scope": "entity.name.tag.html",
				"settings": {
					"foreground": "#28DF99"
				}
			},
			{
				"scope": "punctuation.section.embedded.begin.php",
				"settings": {
					"foreground": "#D2E603"
				}
			},
			{
				"scope": "punctuation.section.embedded.end.php",
				"settings": {
					"foreground": "#D2E603"
				}
			},
			{
				"scope": "source.php",
				"settings": {
					"foreground": "#D2E603"
				}
			},
			{
				"scope": "keyword.control.foreach.php",
				"settings": {
					"foreground": "#7579E7"
				}
			},
			{
				"scope": "keyword.control.endforeach.php",
				"settings": {
					"foreground": "#7579E7"
				}
			},
			{
				"scope": "keyword.control.if.php",
				"settings": {
					"foreground": "#7579E7"
				}
			},
			{
				"scope": "keyword.control.endif.php",
				"settings": {
					"foreground": "#7579E7"
				}
			}
		]
	},
